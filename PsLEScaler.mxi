(c)creative tweet. http://creative-tweet.net
	]]>
	</license-agreement>

	<files>
		<file source="PsLE Scaler.jsf" destination="$fireworksuser/Commands/" />
	</files>

	<configuration-changes>
	</configuration-changes>
	<signatures />

</macromedia-extension>
